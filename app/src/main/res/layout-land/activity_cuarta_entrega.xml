<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".CuartaEntrega">

    <LinearLayout
        android:layout_width="353dp"
        android:layout_height="376dp"
        android:layout_marginStart="41dp"
        android:layout_marginTop="104dp"
        android:layout_marginEnd="42dp"
        android:layout_marginBottom="105dp"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <Button
            android:id="@+id/buttonIS"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:onClick="intent_receiver"
            android:text="@string/buttonIS" />

        <TextView
            android:id="@+id/textViewIS"
            android:layout_width="match_parent"
            android:layout_height="36dp"
            android:text="@string/testViewIS" />

        <Space
            android:layout_width="match_parent"
            android:layout_height="46dp" />

        <Button
            android:id="@+id/buttonS"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:onClick="service_receiver"
            android:text="@string/buttonS" />

        <TextView
            android:id="@+id/textS"
            android:layout_width="match_parent"
            android:layout_height="36dp"
            android:text="@string/textS" />

        <Space
            android:layout_width="match_parent"
            android:layout_height="46dp" />

        <Button
            android:id="@+id/buttonBS"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:onClick="service_bound_receiver"
            android:text="@string/buttonBS" />

        <TextView
            android:id="@+id/textBS"
            android:layout_width="match_parent"
            android:layout_height="36dp"
            android:text="@string/textBS" />

        <Space
            android:layout_width="match_parent"
            android:layout_height="46dp" />

    </LinearLayout>
</androidx.constraintlayout.widget.ConstraintLayout>