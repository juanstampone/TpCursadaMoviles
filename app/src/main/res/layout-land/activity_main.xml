<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <LinearLayout
        android:layout_width="322dp"
        android:layout_height="387dp"
        android:layout_marginStart="41dp"
        android:layout_marginTop="96dp"
        android:layout_marginEnd="42dp"
        android:layout_marginBottom="97dp"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <Space
            android:layout_width="match_parent"
            android:layout_height="71dp" />

        <Button
            android:id="@+id/botonSig"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/PrimeraEntrega" />

        <Space
            android:layout_width="match_parent"
            android:layout_height="21dp" />

        <Button
            android:id="@+id/buttonSegEnt"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/buttonSegEnt" />

        <Space
            android:layout_width="match_parent"
            android:layout_height="21dp" />

        <Button
            android:id="@+id/buttonTerEnt"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/buttonTerEnt" />

        <Space
            android:layout_width="match_parent"
            android:layout_height="21dp" />

        <Button
            android:id="@+id/buttonCuartEnt"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/cuartaEntrega" />

        <Space
            android:layout_width="match_parent"
            android:layout_height="21dp" />

    </LinearLayout>


</androidx.constraintlayout.widget.ConstraintLayout>